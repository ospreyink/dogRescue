<% include partials/header %>
<div class="container">
  <div class="row">
    <div class="col-md-6">
      <div class="card">
        <img class="card-img-top" src="https://farm4.staticflickr.com/3481/3470675106_ab40ab3726.jpg" alt="">
        <div class="card-body">
          <h2 class="card-title">Meet Harley!</h2>
          <p class="card-text">Lorem ipsum dolor sit amet, arcu aptent. Leo elementum pretium mi rem. Cras eleifend feugiat. Hendrerit mus pellentesque pharetra.</p>
          <a href="#" class="btn btn-primary">Learn more!</a>
        </div>
      </div>
    </div>
    <div class="col-md-6">
      <div class="card container">
        <h3>Find your new best friend!</h3>
        <form>
          <div class="form-group">
            <label class="col-form-label" for="formGroupExampleInput">Breed</label>
            <input type="text" class="form-control" id="">
          </div>
          <p>Size</p>
          <div class="form-check form-check-inline">
            <label class="form-check-label">
              <input class="form-check-input" type="checkbox" id="" value=""> S
            </label>
          </div>
          <div class="form-check form-check-inline">
            <label class="form-check-label">
              <input class="form-check-input" type="checkbox" id="" value=""> M
            </label>
          </div>
          <div class="form-check form-check-inline">
            <label class="form-check-label">
              <input class="form-check-input" type="checkbox" id="" value=""> L
            </label>
          </div>
          <div class="form-check form-check-inline">
            <label class="form-check-label">
              <input class="form-check-input" type="checkbox" id="" value=""> XL
            </label>
          </div>
          <p>Age</p>
          <div class="form-check form-check-inline">
            <label class="form-check-label">
              <input class="form-check-input" type="checkbox" id="" value=""> Puppy
            </label>
          </div>
          <div class="form-check form-check-inline">
            <label class="form-check-label">
              <input class="form-check-input" type="checkbox" id="" value=""> Young
            </label>
          </div>
          <div class="form-check form-check-inline">
            <label class="form-check-label">
              <input class="form-check-input" type="checkbox" id="" value=""> Adult
            </label>
          </div>
          <div class="form-check form-check-inline">
            <label class="form-check-label">
              <input class="form-check-input" type="checkbox" id="" value=""> Senior
            </label>
          </div>
          <p><button type="submit" class="btn btn-primary">Fetch!</button></p>
        </form>
      </div>

    </div>
  </div>
  <div class="row">
    <div class="col-md-12">
      <div class="container card">
        <div class="row">
          <h3 class="text-center">Browse other adoptable dogs</h3>
        </div>
        <div class="row">
          <div class="col-md-3">
            <img src="https://farm3.staticflickr.com/2680/4073673270_3c2315fb3c.jpg" class="img-thumbnail">
            <h5>Maisy</h5>
          </div>
          <div class="col-md-3">
            <img src="https://farm9.staticflickr.com/8320/8034482042_ed249192e9.jpg" class="img-thumbnail">
            <h5>Nanook</h5>
          </div>
          <div class="col-md-3">
            <img src="https://farm1.staticflickr.com/91/238957145_485ba97dcc.jpg" class="img-thumbnail">
            <h5>Roger</h5>
          </div>
          <div class="col-md-3">
            <img src="https://farm8.staticflickr.com/7151/6698150783_161b18182e.jpg" class="img-thumbnail">
            <h5>Odie</h5>
          </div>
        </div>
        <div class="row">
          <button class="btn btn-primary">See more!</button>
        </div>

      </div>


    </div>
  </div>
</div>


